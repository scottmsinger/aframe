    },

    "alcove_lm":{
        "node_type": "aframe_asset:image",
        "name": "alcove", 
        "file_src": "image/render/${this.name}/${this.name}_lm.jpg",
        "file_web": "web/assets/image/${this.name}_lm.jpg"
    },

    "alcove_rough":{
        "node_type": "aframe_asset:image",
        "name": "alcove", 
        "file_src": "image/render/${this.name}/${this.name}_rough.jpg",
        "file_web": "web/assets/image/${this.name}_rough.jpg"
    },

    "end": 0
